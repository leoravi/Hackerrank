import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        String str="";
        for(int i=A.length()-1;i>=0;i--)
        {
            str=str+A.charAt(i);
           // str=(A.substring(i-1,i)).concat(str);
        }
        if(str.equals(A))
            System.out.println("Yes");
        else
            System.out.println("No");

    }
}



